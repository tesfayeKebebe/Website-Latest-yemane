    <script>
        window.applyTransformations = (elementId, increment, maxTarget, number) => {
            const element = document.getElementById(elementId);
            if (element) {
                console.log('Element found:', element); // Log to verify element existence
                let currentX = 0;
                let direction = 1; // Start moving right
                let count = 0;
                let intervalId;

                const updateTransform = () => {
                    // Update the position based on the current direction
                    currentX += direction * increment;

                    // Apply the transformation
                    element.style.transition = "transform 0.3s linear";
                    element.style.transform = `translate3d(${currentX}px, 0, 0)`;
                    element.style.transformStyle = "preserve-3d";
                    element.style.willChange = "transform";

                    // Check if the max target is reached and switch direction if needed
                    if (currentX >= maxTarget || currentX <= 0) {
                        direction *= -1; // Reverse the direction
                        //if (currentX <= 0) {
                           // count++;
                        //}

                        // Check if the desired number of cycles is reached
                        //if (count >= number) {
                           // clearInterval(intervalId);
                        //}
                    }
                };

                // Start the interval to update the transformation periodically
                intervalId = setInterval(updateTransform, 30); // Adjust interval time as needed
            } else {
                console.error('Element not found:', elementId); // Log error if element not found
            }
        };
